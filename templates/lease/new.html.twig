{% extends 'base.html.twig' %}

{% block subTitle %}{{ 'title.lease' | trans }}{% endblock %}

{% block bc_content %}
    <twig:breadcrumb:BreadcrumbElement
            label="title.property"
            icon="<i class='fas fa-building'></i>"
            path="{{ path('user_property_index') }}"
    />
    <twig:breadcrumb:BreadcrumbElement
            label="title.lease"
            icon="<i class='fas fa-file-contract'></i>"
    />
    <twig:breadcrumb:BreadcrumbElement
            label="{{ 'form.action.new' }}"
            active="1"
    />
{% endblock %}
{% block body %}
<div class="row justify-content-center">
    <div class="col-8">
        <twig:form:LeaseForm form="{{ form }}" />
    </div>
</div>
{% endblock %}

