
<twig:FormRowCol elements="{{ [
    {
        form: form.number,
        label: 'address.number'
    },
    {
        form: form.numberComplement,
        label: 'address.numberComplement'
    },
    {
        form: form.wayType,
        label: 'address.wayType'
    }
] }}" />
<twig:FormRow form="{{ form.wayName }}" label="address.wayName" />
<twig:FormRow form="{{ form.complement }}" label="address.complement" />
<twig:FormRowCol elements="{{ [
    {
        form: form.zip,
        label: 'address.zip'
    },
    {
        form: form.city,
        label: 'address.city'
    }
] }}" />